<!DOCTYPE HTML>
<html>

<body>



    <div class="wrap">
        <div class="about">
            <h3>About</h3>
            @foreach (var item in Model)
            {
                <img src="@item.ImageUrl" alt="" />
                <p>@item.Description</p>

            }

        </div>
    </div>


</body>
</html>




